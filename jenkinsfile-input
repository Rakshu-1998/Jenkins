pipeline{
    agent any 
    stages {
        stage('EXAMPLE'){
            input{
               message Hi,I am Rakhistha from Wipro
                can we connect now?
                submitter "Rakshitha"
                paramaters{
                    string (name: 'PERSON', defaultValue: 'Ms Rakshitha', description: 'Who should I say hello to?')
                }
            }
             steps {
                echo "Hello, ${PERSON}, nice to meet you."
        }
    }
}
}
