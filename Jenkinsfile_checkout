pipeline{
    agent any
    stages{
        stage('checkout stage'){
            steps{
                sh 'ls -lrt'
            }
        }
        stage( 'build'){
            steps{
                sh 'pwd'
            }
        }
        stage('job'){
            steps{
                sh 'cd'
            }
        }
        stage( 'job2'){
            steps{
                sh 'ls -a'
            }
        }
     }

    }