pipeline {
    agnet any
    stages {
        stage('FIRST_STAGE'){
            steps{
                echo "This is the first stage"
            }
        }
        stage('SECONF_STAGE'){
            steps{
                echo "This is seconf stage"
            }
        }
        post {
            always {
                echo "This will run on when stage is SUCESS, FAILURE, UNSTABLE, ABORTED"
            }
            failure {
                echo "This will run when build fails"
            }
        }
    }
}